{
  "manifest_version": 3,
  "name": "PM Tool Manager",
  "version": "1.0",
  "description": "Extension para gestionar herramientas de Web Project Manager",
  "icons": {
    "48": "icons/avantio.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "action": {
    "default_icon": "icons/avantio.png",
    "default_title": "PM Tool Manager",
    "default_popup": "popup/popup.html"
  }
}